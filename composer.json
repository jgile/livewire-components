{
    "name": "jgile/livewire-components",
    "description": "livewire-components",
    "keywords": [
        "jgile",
        "livewire-components"
    ],
    "homepage": "https://github.com/jgile/livewire-components",
    "type": "laravel",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "John Gile",
            "email": "me@jgile.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "JGile\\LivewireComponents\\": "src",
            "JGile\\LivewireComponents\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JGile\\LivewireComponents\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JGile\\LivewireComponents\\LivewireComponentsServiceProvider"
            ]
        }
    }
}
